{
  "name": "www.weboftomorrow.com",
  "version": "0.4.0",
  "description": "A statically generated website",
  "private": true,
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "css-byebye": "^1.0.1",
    "css-loader": "^0.23.1",
    "cssnano": "^3.5.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "postcss-calc": "^5.2.1",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.0",
    "postcss-import": "^8.1.0",
    "postcss-loader": "^0.8.2",
    "postcss-nested": "^1.0.0",
    "postcss-reporter": "^1.3.3",
    "postcss-url": "^5.1.1",
    "standard": "^6.0.8",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0"
  },
  "scripts": {
    "pretest": "standard",
    "prebuild": "rm -rf dist",
    "build": "webpack --optimize-minimize --optimize-occurence-order",
    "preinstall": "npm run build",
    "debug": "webpack --debug --devtool source-map --output-pathinfo",
    "watch": "webpack --debug --devtool source-map --output-pathinfo --watch",
    "preversion": "npm test && npm run build",
    "version": "git add -f dist && git commit -m 'Add dist/ files'",
    "postversion": "chill freeze && git push --all && chill-upload-rackspace.py"
  },
  "standard": {
    "ignore": [
      "node_modules/*",
      "dist",
      "webpack.config.js"
    ]
  },
  "author": "Jake Hickenlooper",
  "license": "WTFPL",
  "dependencies": {
    "suitcss-utils-text": "file:lib/suitcss-utils-text-1.0.0.tgz"
  }
}
