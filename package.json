{
  "name": "www.weboftomorrow.com",
  "version": "0.2.0",
  "description": "A statically generated website",
  "private": true,
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.5.2",
    "css-byebye": "^1.0.1",
    "css-loader": "^0.23.1",
    "cssnano": "^3.0.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "postcss-calc": "^5.0.0",
    "postcss-custom-media": "^5.0.0",
    "postcss-custom-properties": "^5.0.0",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.8.0",
    "postcss-nested": "^1.0.0",
    "postcss-reporter": "^1.2.1",
    "postcss-url": "^5.1.1",
    "standard": "^6.0.7",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9"
  },
  "scripts": {
    "pretest": "standard",
    "prebuild": "rm -rf dist",
    "build": "webpack --optimize-minimize --optimize-occurence-order",
    "preinstall": "npm run build",
    "debug": "webpack --debug --devtool source-map --output-pathinfo",
    "watch": "webpack --debug --devtool source-map --output-pathinfo --watch",
    "preversion": "npm test && npm run build",
    "version": "chill freeze",
    "postversion": "git push --all && chill-upload-rackspace.py"
  },
  "standard": {
    "ignore": [
      "node_modules/*",
      "dist",
      "webpack.config.js"
    ]
  },
